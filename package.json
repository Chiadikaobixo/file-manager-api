{
  "name": "file-manager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "db:seed": "SET NODE_ENV=development & npx prisma db seed",
    "db:reset": "del .files && NODE_ENV=development & npx prisma migrate reset",
    "dev": "NODE_ENV=development & nodemon app.ts",
    "start": "tsc --build && node dist/app.js",
    "start:clean": "rm -rf dist && tsc --build && node dist/app.js",
    "format": "prettier . --write",
    "lint": "eslint . --fix --ext .ts,.js --max-warnings 0",
    "type-check": "tsc -p .",
    "build": "tsc --build",
    "build:clean": "rm -rf dist && tsc --build"
  },
  "prisma":{
    "seed": "ts-node seed.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/luxon": "^2.3.2",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.121.0",
    "@aws-sdk/s3-request-presigner": "^3.121.0",
    "@prisma/client": "^4.0.0",
    "aws-sdk": "^2.1175.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.5.0",
    "graphql-modules": "^2.1.0",
    "luxon": "^3.0.1",
    "uuidv4": "^6.2.13"
  }
}
